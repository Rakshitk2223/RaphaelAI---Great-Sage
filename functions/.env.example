# Environment variables for Raphael AI
# Copy this file to .env and fill in your actual values
# NEVER commit the actual .env file to version control

# Google Cloud Project Configuration
GOOGLE_APPLICATION_CREDENTIALS=path/to/your/firebase-service-account-key.json
GOOGLE_CLOUD_PROJECT=raphael-great-sage

# Firebase Admin SDK Service Account Key
FIREBASE_SERVICE_ACCOUNT_KEY=path/to/your/firebase-service-account-key.json

# Gemini AI API Key
GEMINI_API_KEY=your-gemini-api-key

# Flask Configuration
FLASK_ENV=development
FLASK_DEBUG=True
PORT=8080

# Firebase Configuration (already configured in frontend)
# Project ID: raphael-great-sage
# Auth Domain: raphael-great-sage.firebaseapp.com
